{
  "name": "teidaishu-api",
  "main": "src/index.js",
  "compatibility_date": "2025-12-17",
  "workers_dev": false,
  "preview_urls": false,

  "vectorize": [
    {
      "binding": "VEC",
      "index_name": "open-run-teidaishu-reddit-ja"
    }
  ],

  "r2_buckets": [
    {
      "binding": "STAGED",
      "bucket_name": "open-run-teidaishu"
    }
  ],

  "vars": {
    "R2_PREFIX": "staged",
    "GEMINI_EMBED_MODEL": "gemini-embedding-001",
    "GEMINI_EMBED_DIM": "1536",
    "GEMINI_EMBED_TASK_TYPE_QUERY": "RETRIEVAL_QUERY",
    "GEMINI_EMBED_TASK_TYPE_DOC": "RETRIEVAL_DOCUMENT",
    "GEMINI_GEN_MODEL": "gemini-2.5-flash"
  }
}
